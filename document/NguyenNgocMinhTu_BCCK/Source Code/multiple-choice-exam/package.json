{
  "name": "multiple-choice-exam",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next build && next export",
    "build:local": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next build",
    "start": "next start",
    "size": "node --max_old_space_size=4096",
    "fix:prettier": "prettier --write . '!.next' '!.node_modules' '!tools/output/.next'",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "pretty": "prettier --write . --ignore-path .gitignore",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx . --ignore-path .gitignore",
    "formatter": "npm-run-all pretty lint:fix",
    "typeorm": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js",
    "schema:drop": "npm run typeorm schema:drop"
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "prettier --write --ignore-unknown"
    ],
    "*.+(json|css|md)": [
      "prettier --write"
    ]
  },
